{
  "name": "clianna-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct-update": "playwright test -c playwright-ct.config.ts --update-snapshots",
    "test-ct-debug": "playwright test -c playwright-ct.config.ts --debug",
    "convert-config": "./convert-env-to-config.sh",
    "create-api-client": "swagger-typescript-api -p http://localhost:4001/swagger/v1/swagger.json -o ./utils/api/generated/temp -n Api.ts -t ./utils/api/templates --api-class-name Client --unwrap-response-data --disable-throw-on-error --custom-config ./utils/api/customConfig.js && echo '// @ts-nocheck' | cat - ./utils/api/generated/temp/Api.ts > ./utils/api/generated/Api.ts && rm -rf ./utils/api/generated/temp"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@google-cloud/local-auth": "^2.1.0",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.97",
    "@mui/material": "^5.10.3",
    "@mui/x-data-grid": "^5.17.0",
    "babel-plugin-superjson-next": "^0.4.4",
    "dayjs": "^1.11.5",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "next-auth": "^4.23.1",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.18.0",
    "ts-enum-util": "^4.0.2"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.26.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-highlight-words": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "next-router-mock": "^0.7.4",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "4.8.2",
    "vite-tsconfig-paths": "^3.5.1"
